      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Process Values For Display
if( game_debug_mode ){

        //Break all values out based on category.
    var _ds_output = ds_map_create(),
        _ds_keys,
        _key_found,
        _ds_keys_len,
        i, j;
        
        _ds_keys[0] = "General";
        
        debug = ""
        
    //Populate the Map
    for( i = 1; i &lt; array_height_2d( game_debug_values ); i++ ){
        _key_found = false; 
    
        if( is_undefined( _ds_output[? game_debug_values[i,0]] ) ) _ds_output[? game_debug_values[i,0]] = ""; 
        
        _ds_output[? game_debug_values[i,0]] += string( game_debug_values[i,1] );
        
        _ds_keys_len = array_length_1d( _ds_keys ); 
        
        for( j = 0; j &lt; _ds_keys_len; j++ ){
            //If already in array, stop. 
            if( _ds_keys[j] == game_debug_values[i,0] ){
                _key_found = true; 
                break;
            }
        }
        if( !_key_found ){
            _ds_keys[ _ds_keys_len ] = game_debug_values[i,0]; 
        }
    }
    
    //Prepare for Drawing
    debug = game_debug_title + "##"; 

    for( i=0; i &lt; array_length_1d( _ds_keys ); i++ ){
        if( !is_undefined(  _ds_output[? _ds_keys[i] ] ) ){
            debug += "  " + string(_ds_keys[i]) + "#" + string(_ds_output[? _ds_keys[i] ]) + "#";  
        }     
    }
    
    ds_map_destroy( _ds_output );

}
</string>
          </argument>
        </arguments>
      </action>
