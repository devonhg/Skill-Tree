        draw_set_alpha( .5 );
            draw_rectangle( 0, 0, display_get_gui_width(), console_size, false );
            draw_rectangle( 0, console_size, display_get_gui_width(), console_size - 4, false );
        draw_set_alpha( 1 );
    draw_set_color( c_white );
 
    //Draw
    draw_text(36,50,
        debug
    );

    console_size = string_height( debug ) + 100; 
}
